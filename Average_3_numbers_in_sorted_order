#include<iostream>
using namespace std;
int main(){
	int a,b,c;
	cin>>a>>b>>c;
	if(a<b){
		if(b<c){
		cout<<a<<b<<c;
				}
		else{
			if(c<a){
				cout<<c<<a<<b;
					}
				else
					cout<<a<<c<<b;
	}	
}
else{
	if(a<c){
		cout<<b<<a<<c;
			}
	else{
		if(c<b){
			cout<<c<<b<<a;
				}
			else
			cout<<b<<c<<a;
		}
	}
}
